                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        T.fecha AS Fecha, T.hora AS Hora, M.nombre AS NombreM, M.apellido AS Medico, E.descripcion AS Especialidad, P.descripcion AS Practica, P.costo AS Costo, P.costo - P.costo * (M.porcentajeDescuento / 100) AS Importe, 
                         Pa.nombre AS NombreP, Pa.apellido AS ApellidoP
FROM            Pacientes AS Pa INNER JOIN
                         Turnos AS T ON Pa.dni = T.paciente INNER JOIN
                         Especialidades AS E ON T.idEspecialidad = E.idEspecialidad INNER JOIN
                         Practicas AS P ON T.idPractica = P.idPractica INNER JOIN
                         Medicos AS M ON T.medico = M.legajo
WHERE        (T.fecha = CONVERT(date, GETDATE())) AND (T.medico = @Medico) AND (T.confirmado = 1) AND (T.activo = 1)</CommandText>
