                         Pacientes AS P ON T.paciente = P.dni INNER JOIN
                         Especialidades AS E ON T.idEspecialidad = E.idEspecialidad ON X.idPractica = T.idPractica
WHERE        (T.fecha = CONVERT(date, GETDATE())) AND (T.confirmado = 1)</CommandText>
